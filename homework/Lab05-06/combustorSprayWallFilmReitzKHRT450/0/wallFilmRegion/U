FoamFile
{
    format      ascii;
    class       volVectorField;
    object      Uf;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    "(wallFilmFaces_top|fuel_inlet)"
    {
        type            slip;
    }

    region0_to_wallFilmRegion_wallFilmFaces
    {
        type            noSlip;
    }

    frontAndBack
    {
        type            empty;
    }
}

