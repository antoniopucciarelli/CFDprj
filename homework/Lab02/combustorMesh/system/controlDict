// this dictionary sets the different steps to follow in order to solve the case 
FoamFile
{
    format      ascii;
    class       dictionary;
    object      controlDict;
}

application     icoFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         500;

deltaT          1;

writeControl    timeStep;

writeInterval   100;

purgeWrite      0;

writeFormat     binary;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;
